{
  "name": "one-sql-client",
  "version": "0.0.1",
  "description": "SQL Workspace",
  "main": ".vite/build/main.js",
  "scripts": {
    "webapp:dev": "vite",
    "webapp:build": "npx tsc && vite build",
    "desktop:start": "electron-forge start",
    "desktop:package": "electron-forge package",
    "desktop:make": "electron-forge make && mkdir -p ./build && mv -f out ./build/desktop",
    "desktop:publish": "electron-forge publish",
    "coverage": "vitest run --coverage",
    "previews": "vite --config src/__tests__/previews/previews.vite.config.ts",
    "lint": "eslint --ext .js,.ts,.tsx src --report-unused-disable-directives --max-warnings 0",
    "clean": "rm -rf build/",
    "test": "vitest",
    "test-ui": "vitest --ui",
    "pre-commit": "npm run lint && npx tsc && npm run test -- --run"
  },
  "config": {
    "forge": "src/desktop/forge.config.js"
  },
  "author": "Philippe Shampanier",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-vite": "^7.2.0",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.16",
    "electron": "^28.0.0",
    "eslint": "^8.55.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "happy-dom": "^13.0.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.1.6",
    "vite-plugin-svgr": "^4.2.0",
    "vite-plugin-windicss": "^1.9.2",
    "vite-tsconfig-paths": "^4.2.2",
    "vitest": "^1.0.2"
  },
  "dependencies": {
    "classix": "^2.1.35",
    "electron-squirrel-startup": "^1.0.0",
    "github-markdown-css": "^5.4.0",
    "monaco-editor": "^0.44.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-monaco-editor": "^0.55.0",
    "reflect-metadata": "^0.1.14",
    "remark-gfm": "^4.0.0",
    "zustand": "^4.4.7"
  }
}
